<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
			  name="subreport_locacao_por_veiculo" language="java" pageWidth="555" pageHeight="802" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">

	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="One Empty Record"/>

	<queryString>
		<![CDATA[]]>
	</queryString>

	<field name="codigo" class="java.lang.Long"/>
	<field name="nomeCliente" class="java.lang.String"/>
	<field name="dataInicio" class="java.time.LocalDateTime"/>
	<field name="dataFim" class="java.time.LocalDateTime"/>
	<field name="valorPago" class="java.math.BigDecimal"/>
	<field name="formaPagamento" class="java.lang.String"/>
	<field name="statusLocacao" class="java.lang.String"/>

	<columnHeader>
		<band height="25">
			<staticText>
				<reportElement mode="Opaque" x="0" y="0" width="555" height="25" backcolor="#F0F0F0"/>
				<box topPadding="2" leftPadding="5" bottomPadding="2" rightPadding="5"/>
				<textElement verticalAlignment="Middle">
					<font isBold="true"/>
				</textElement>
				<text><![CDATA[Detalhes das Locações]]></text>
			</staticText>
		</band>
	</columnHeader>

	<detail>
		<band height="30">
			<textField>
				<reportElement x="0" y="0" width="40" height="30"/>
				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{codigo}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="40" y="0" width="120" height="30"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{nomeCliente}]]></textFieldExpression>
			</textField>
			<textField pattern="dd/MM/yyyy">
				<reportElement x="160" y="0" width="80" height="30"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{dataInicio}]]></textFieldExpression>
			</textField>
			<textField pattern="dd/MM/yyyy">
				<reportElement x="240" y="0" width="80" height="30"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{dataFim}]]></textFieldExpression>
			</textField>
			<textField pattern="¤ #,##0.00">
				<reportElement x="320" y="0" width="80" height="30"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{valorPago}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="400" y="0" width="80" height="30"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{formaPagamento}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="480" y="0" width="75" height="30"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{statusLocacao}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>